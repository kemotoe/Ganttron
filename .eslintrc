{
  "extends": "eslint-config-airbnb",
  "rules": {
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": [2, { "ignore": ["electron"] }],
    "linebreak-style": 0,
    "one-var": 0,
    "no-param-reassign": 0,
    "no-unused-vars": 0,
    "no-undef": 0,
    "new-cap": 0,
    "consistent-return": 0,
    "no-multi-assign": 0
  },
  "globals": {
    "document": true,
    "window": true
  }
}
